<h1 class="page-header text-center"> Add a Contact </h1>

<form id="add_contact" action="/contact_list" method="post">
  <div class="form-group">
    <label for="first_name">First Name:</label>
    <input id="last_name" type="text" name="first_name">

    <label for="last_name">Last Name:</label>
    <input id="last_name" type="text" name="last_name">
  </div>
  <button name="add_contact" type="submit" class="btn btn-success">Save</button>
</form>
